"*":
  welcome:
    showOnStartup: false
  editor:
    fontFamily: "Consolas, Inconsolata, Monaco, Courier New, Courier"
    fontSize: 14
    invisibles:
      tab: "â†’"
    showInvisibles: true
  core:
    themes: [
      "neutron-ui"
      "leaf-syntax"
    ]
    disabledPackages: [
      "metrics"
      "tabs"
      "tree-view"
    ]
  tabs:
    showIcons: true
  "tree-view":
    hideVcsIgnoredFiles: false
    hideIgnoredNames: false
    showOnRightSide: false
    sortFoldersBeforeFiles: true
  "file-icons":
    coloured: false
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
