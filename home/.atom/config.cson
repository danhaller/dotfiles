"*":
  welcome:
    showOnStartup: false
  editor:
    fontSize: 12
    fontFamily: "Droid Sans Mono, Monaco, Inconsolata, Consolas, Courier New, Courier"
    tabLength: 4
    softTabs: false
    invisibles:
      cr: ""
      eol: ""
      tab: "â†’"
    showInvisibles: true
  core:
    themes: [
      "neutron-ui"
      "leaf-syntax"
    ]
    disabledPackages: [
      "metrics"
      "exception-reporting"
    ]
  tabs:
    showIcons: true
  "file-icons":
    coloured: false
  "linter-eslint":
    disableWhenNoEslintrcFileInPath: true
  "linter-jscs":
    onlyConfig: true
  linter: {}
  "atom-ternjs": {}
  "atom-color-highlight": {}
  "tree-view":
    hideVcsIgnoredFiles: true
  "ide-haskell": {}
  "language-elm": {}
